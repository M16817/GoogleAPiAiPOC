    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:BotHandlerUWP.Usercontrols"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400" Height="30" Width="30" Background="Transparent">

    <Grid Height="30" Width="30" Background="Transparent">
        <Image x:Name="ProfileImg" Source="ms-appx:///Assets/6422482.png" Stretch="UniformToFill" Height="30" Width="30"/>
    </Grid>
</UserControl>
